{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "juego3dvs1": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "www",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/leaflet/dist/images",
                "output": "/assets/leaflet/images"
              },
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              }
            ],
            "styles": [
              "node_modules/leaflet/dist/leaflet.css",
              "src/styles.css",
              {
                "input": "node_modules/ngx-toastr/toastr.css",
                "bundleName": "toastr",
                "inject": true
              }
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "canvg",
              "core-js",
              "leaflet",
              "rgbcolor",
              "raf",
              "jspdf",
              "html2canvas",
              "cannon",
              "stats.js"
            ],
            "optimization": true,
            "vendorChunk": true,
            "extractLicenses": true,
            "buildOptimizer": true,
            "sourceMap": false,
            "namedChunks": false,
            "aot": true,
            "outputHashing": "all"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "40kb",
                  "maximumError": "60kb"
                }
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": true,
              "extractLicenses": false,
              "buildOptimizer": false,
              "namedChunks": true,
              "aot": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "juego3dvs1:build:production"
            },
            "development": {
              "buildTarget": "juego3dvs1:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "cache": {
      "enabled": true,
      "path": ".cache",
      "environment": "all"
    }
  }
}